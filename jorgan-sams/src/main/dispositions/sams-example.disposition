      <references/>
      <messages/>
      <zoom>1.0</zoom>
    </label>
    <sams.samsConsole id="1205">
      <name></name>
      <description></description>
      <references>
        <console-locationReference id="1204">
          <x>60</x>
          <y>100</y>
        </console-locationReference>
