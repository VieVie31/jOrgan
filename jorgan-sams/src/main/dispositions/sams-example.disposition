<?xml version="1.0" encoding="UTF-8" ?>
<organ version="@jorgan.version@">
  <elements>
    <switch id="1202">
      <name></name>
      <description></description>
      <references/>
      <messages>
        <switch-activated>
          <status>set 144</status>
          <data1>set 60</data1>
          <data2>set 127</data2>
        </switch-activated>
        <switch-deactivated>
          <status>set 128</status>
          <data1>set 60 </data1>
          <data2>set 127</data2>
        </switch-deactivated>
      </messages>
      <zoom>1.0</zoom>
      <active>false</active>
      <duration>-1</duration>
