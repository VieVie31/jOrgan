<?xml version="1.0" encoding="UTF-8" ?>
<mapping>
  <name>Test</name>
  <device>jOrgan Keyboard</device>
  <direction>IN</direction>
  <mappers>
    <mapper>
      <from>
        <message>
          <datas>
            <string>equal 144</string>
            <string>get pitch</string>
            <string></string>
          </datas>
        </message>
      </from>
      <to>
        <message>
          <datas>
            <string>set 144</string>
            <string>set pitch | add 12</string>
            <string>set 100</string>
          </datas>
        </message>
      </to>
    </mapper>
    <mapper>
      <from>
        <message>
          <datas>
            <string>equal 128</string>
            <string>get pitch</string>
            <string></string>
          </datas>
        </message>
      </from>
      <to>
        <message>
          <datas>
            <string>set 128</string>
            <string>set pitch | add 12</string>
            <string></string>
          </datas>
        </message>
      </to>
    </mapper>
  </mappers>
</mapping>